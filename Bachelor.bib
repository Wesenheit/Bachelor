	note = {arXiv:2006.16951 [astro-ph]},
	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Instrumentation and Methods for Astrophysics, Astrophysics - Earth and Planetary Astrophysics},
	pages = {34},
	annote = {Comment: accepted for publication in ApJS},
	file = {arXiv Fulltext PDF:/home/mitra/Zotero/storage/KSANPA96/Conroy et al. - 2020 - Physics of Eclipsing Binaries. V. General Framewor.pdf:application/pdf;arXiv.org Snapshot:/home/mitra/Zotero/storage/PZJHWQ5X/2006.html:text/html},
