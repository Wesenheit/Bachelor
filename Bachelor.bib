@article{el-badry_what_2022,
	title = {What are the spectroscopic binaries with high-mass functions near the \textit{{Gaia}} {DR3} main sequence?},
	volume = {515},
	issn = {0035-8711, 1365-2966},
	url = {https://academic.oup.com/mnras/article/515/1/1266/6620842},
	doi = {10.1093/mnras/stac1797},
	abstract = {ABSTRACT
            The third data release of the Gaia mission includes orbital solutions for \&gt;105 single-lined spectroscopic binaries, representing more than an order of magnitude increase in sample size over all previous studies. This data set is a treasure trove for searches for quiescent black hole + normal star binaries. We investigate one population of black hole candidate binaries highlighted in the data release: sources near the main sequence in the colour–magnitude diagram (CMD) with dynamically inferred companion masses M2 larger than the CMD-inferred mass of the luminous star. We model light curves, spectral energy distributions, and archival spectra of the 14 such objects in DR3 with high-significance orbital solutions and inferred \$M\_2 {\textbackslash}gt 3{\textbackslash}, \{{\textbackslash}rm M\}\_\{{\textbackslash}odot \}\$. We find that 100 per cent of these sources are mass-transfer binaries containing a highly stripped lower giant donor (0.2 ≲ M/M⊙ ≲ 0.4) and much more massive (2 ≲ M/M⊙ ≲ 2.5) main-sequence accretor. The Gaia orbital solutions are for the donors, which contribute about half the light in the Gaia RVS bandpass but only \${\textbackslash}lesssim 20\{\{{\textbackslash} {\textbackslash}rm per{\textbackslash} cent\}\}\$ in the g band. The accretors’ broad spectral features likely prevented the sources from being classified as double-lined. The donors are all close to Roche lobe filling (\$R/R\_\{{\textbackslash}rm Roche{\textbackslash}, lobe\}{\textbackslash}gt 0.8\$), but modelling suggests that a majority are detached (\$R/R\_\{{\textbackslash}rm Roche{\textbackslash}, lobe\}{\textbackslash}lt 1\$). Binary evolution models predict that these systems will soon become detached helium white dwarf + main-sequence ‘EL CVn’ binaries. Our investigation highlights both the power of Gaia data for selecting interesting subpopulations of binaries and the ways in which binary evolution can bamboozle standard CMD-based stellar-mass estimates.},
	language = {en},
	number = {1},
	urldate = {2023-01-02},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {El-Badry, Kareem and Rix, Hans-Walter},
	month = jul,
	year = {2022},
	pages = {1266--1275},
	file = {Submitted Version:/home/mitra/Zotero/storage/EV8GIT7G/El-Badry and Rix - 2022 - What are the spectroscopic binaries with high-mass.pdf:application/pdf},
}

