@article{jayasinghe_unicorn_2021-1,
	title = {A unicorn in monoceros: the 3 {M}⊙ dark companion to the bright, nearby red giant {V723} {Mon} is a non-interacting, mass-gap black hole candidate},
	volume = {504},
	issn = {0035-8711, 1365-2966},
	shorttitle = {A unicorn in monoceros},
	url = {https://academic.oup.com/mnras/article/504/2/2577/6261635},
	doi = {10.1093/mnras/stab907},
	abstract = {ABSTRACT
            We report the discovery of the closest known black hole candidate as a binary companion to V723 Mon. V723 Mon is a nearby (\$d{\textbackslash}sim 460{\textbackslash}, {\textbackslash}rm pc\$), bright (V ≃ 8.3 mag), evolved (Teff, giant ≃ 4440 K, and Lgiant ≃ 173 L⊙) red giant in a high mass function, f(M) = 1.72 ± 0.01 M⊙, nearly circular binary (P = 59.9 d, e ≃ 0). V723 Mon is a known variable star, previously classified as an eclipsing binary, but its All-Sky Automated Survey, Kilodegree Extremely Little Telescope, and Transiting Exoplanet Survey Satellite light curves are those of a nearly edge-on ellipsoidal variable. Detailed models of the light curves constrained by the period, radial velocities, and stellar temperature give an inclination of \$87.0{\textasciicircum}\{{\textbackslash}circ {\textasciicircum}\{+1.7{\textasciicircum}{\textbackslash}circ \}\}\_\{-1.4{\textasciicircum}{\textbackslash}circ \}\$, a mass ratio of q ≃ 0.33 ± 0.02, a companion mass of Mcomp = 3.04 ± 0.06 M⊙, a stellar radius of Rgiant = 24.9 ± 0.7 R⊙, and a giant mass of Mgiant = 1.00 ± 0.07 M⊙. We identify a likely non-stellar, diffuse veiling component with contributions in the B and V band of \$\{{\textbackslash}sim \}63\{\{{\textbackslash} {\textbackslash}rm per{\textbackslash} cent\}\}\$ and \$\{{\textbackslash}sim \}24\{\{{\textbackslash} {\textbackslash}rm per{\textbackslash} cent\}\}\$, respectively. The SED and the absence of continuum eclipses imply that the companion mass must be dominated by a compact object. We do observe eclipses of the Balmer lines when the dark companion passes behind the giant, but their velocity spreads are low compared to observed accretion discs. The X-ray luminosity of the system is \$L\_\{{\textbackslash}rm X\}{\textbackslash}simeq 7.6{\textbackslash}times 10{\textasciicircum}\{29\}{\textasciitilde}{\textbackslash}rm ergs{\textasciitilde}s{\textasciicircum}\{-1\}\$, corresponding to L/Ledd ∼ 10−9. The simplest explanation for the massive companion is a single compact object, most likely a black hole in the ‘mass gap’.},
	language = {en},
	number = {2},
	urldate = {2023-01-02},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Jayasinghe, T and Stanek, K Z and Thompson, Todd A and Kochanek, C S and Rowan, D M and Vallely, P J and Strassmeier, K G and Weber, M and Hinkle, J T and Hambsch, F-J and Martin, D V and Prieto, J L and Pessi, T and Huber, D and Auchettl, K and Lopez, L A and Ilyin, I and Badenes, C and Howard, A W and Isaacson, H and Murphy, S J},
