	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Astrophysics of Galaxies},
	pages = {A128},
	annote = {Comment: 12 pages, 12 figures, accepted in A\&A},
	file = {arXiv Fulltext PDF:/home/mitra/Zotero/storage/XAHX9Z6T/Costa et al. - 2019 - Multiple stellar populations in NGC 1866. New clue.pdf:application/pdf;arXiv.org Snapshot:/home/mitra/Zotero/storage/S26XMQN9/1909.html:text/html},
